
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Would charge</th>
      <th>Would pay</th>
      <th>Timestamp</th>
    </tr>
  </thead>

  <tbody>
  <% @surveys.each do |survey| %>
    <tr>
      <td><%= survey.email %></td>
      <td><%= survey.would_charge %></td>
      <td><%= survey.would_pay %></td>
      <td><%= survey.created_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>


